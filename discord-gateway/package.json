{
  "name": "@aimaestro/discord-gateway",
  "version": "0.1.0",
  "description": "Discord gateway for AI Maestro - routes Discord messages to AI Maestro agents via discord.js",
  "main": "src/server.ts",
  "type": "module",
  "scripts": {
    "start": "tsx src/server.ts",
    "dev": "tsx watch src/server.ts",
    "build": "tsc",
    "typecheck": "tsc --noEmit",
    "test": "node --import tsx --test src/__tests__/*.test.ts"
  },
  "dependencies": {
    "discord.js": "^14.16.0",
    "dotenv": "^16.6.0",
    "express": "^4.18.2"
  },
  "devDependencies": {
    "@types/express": "^5.0.0",
    "@types/node": "^22.10.0",
    "tsx": "^4.19.0",
    "typescript": "^5.7.0"
  }
}
